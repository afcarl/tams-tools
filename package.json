{
  "name": "kvgenerator-cycle",
  "version": "0.1.0",
  "description": "Tool to generate and edit KV Diagrams",
  "main": "index.js",
  "private": true,
  "scripts": {
    "compile": "env NODE_ENV=production webpack --config webpack/prod.config.babel.js --progress",
    "lint": "eslint .",
    "start": "webpack-dev-server --config webpack/dev.config.babel.js --progress --inline --hot --host 0.0.0.0",
    "test": "ava",
    "clear": "rm -rf ./build"
  },
  "author": "Laszlo Korte <me@laszlokorte.de> (https://www.laszlokorte.de/)",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.13.0",
    "babel": "^6.5.2",
    "babel-core": "^6.7.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-function-bind": "^6.5.2",
    "babel-preset-es2015": "^6.6.0",
    "clean-webpack-plugin": "^0.1.8",
    "compression-webpack-plugin": "0.2.0",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "eslint": "2.2.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-no-class": "^0.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^2.10.0",
    "json-loader": "^0.5.4",
    "pegjs": "^0.9.0",
    "pegjs-loader": "^0.4.0",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.5.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.8.3"
  },
  "dependencies": {
    "@cycle/core": "^6.0.3",
    "@cycle/dom": "^9.1.0",
    "@cycle/isolate": "^1.2.0",
    "@cycle/storage": "^2.0.3",
    "babel-polyfill": "^6.7.2",
    "bitset.js": "^3.1.0",
    "immutable": "^3.7.6",
    "nib": "^1.1.0",
    "raf": "^3.2.0",
    "rx": "^4.1.0",
    "virtual-dom": "^2.1.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "ava": {
      "files": [
        "**/*.spec.js"
      ],
      "source": [
        "**/*.{js,jsx}",
        "!build/**/*",
        "!**/*.spec.js"
      ],
      "failFast": true,
      "tap": false,
      "require": [
        "babel-register"
      ],
      "babel": "inherit"
    }
}
